    </NativeReference>

    <NativeReference Include="Frameworks/flutter_icmp_ping.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>False</SmartLink>
      <ForceLoad>True</ForceLoad>
      <LinkerFlags>-ObjC</LinkerFlags>
      <WeakFrameworks>Flutter</WeakFrameworks>
    </NativeReference>

    <NativeReference Include="Frameworks/geolocator_apple.xcframework">
